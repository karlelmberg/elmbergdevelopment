@model Web.Models.Pages.ArticlePage

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <h1>@Model.Heading</h1>
        <div>@Model.Ingress</div> 
        <div>
            @if (!string.IsNullOrEmpty(Model.Image.ImageUrl))
            {
                @Model.Image.ToHtml()
            }
        </div>
        <div>@Model.MainBody.Value</div>
    </div>
</div>